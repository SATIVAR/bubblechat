{
  "name": "@bubblle/api",
  "version": "1.0.0",
  "description": "Bubblle Agent CRM - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env PORT=3000 nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "start": "cross-env PORT=3000 node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "tesseract.js": "^5.0.3",
    "pdf-parse": "^1.1.1",
    "xlsx": "^0.18.5",
    "socket.io": "^4.7.4",
    "openai": "^4.20.1",
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "prisma": "^5.7.0",
    "@prisma/client": "^5.7.0",
    "redis": "^4.6.10",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "express-async-errors": "^3.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "crm",
    "llm",
    "chat",
    "ai",
    "multi-tenant"
